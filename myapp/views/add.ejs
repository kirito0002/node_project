<%- include ./common/add.ejs  %>

<div class="container">
    <div class="row">
        <div class="col-sm-10" style="margin-top:50px;">
            <div class="panel panel-default">
                <div class="panel-heading">
                    添加新病人
                </div>
                <div class="panel-body">
                    <div class="table-responsive input-form">
                        <form action="http://localhost:4000/add" method="post" enctype="multipart/form-data">
                            <ul>
                                <li> 病人名称: <input type="text" name="title" /></li>

                                <li> 病人图片: <input type="file" name="pic" /></li>
                                <li> 病房号: <input type="text" name="price" /></li>
                                <li> 健康状态: <input type="text" name="fee" /></li>
                                <li> 入院时间: <input type="text" name="createAt"/></li>
                                <li>
                                    <br />
                                    <button type="submit" class="btn btn-default">提交</button>
                                </li>

                            </ul>
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<%- include ./common/footer.ejs  %>